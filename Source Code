            char A2 = 'p';
            char A3 = 's';
            char A4 = 'e';
            char A5 = 't';
            char a1,a2,a3,a4,a5;
            Begin = clock();
            printf("\nupset\a");
            for( ;clock() - Begin < Dispaly*CLOCKS_PER_SEC; );
            {
                printf("\r");
            }
            for(int x = 1;x <= Qua1;++x)
            {
                printf(" ");
            }
            if(Tries==1)
            {
                printf("Now input the word.");
            }
            else
            {
                printf("\r");
            }
            scanf("%c %c %c %c %c",&a1,&a2,&a3,&a4,&a5);
            ++Tries;
            if(tolower(a1)==A1&&tolower(a2)==A2&&tolower(a3)==A3&&tolower(a4)==A4&&tolower(a5)==A5)
            {
                printf("Corecct!Well done.\n\a");
                ++Mark;
                goto Core;
            }
            else
            {
                printf("Wrong!It\'s a shame.");
                printf("Your final mark is %d",Mark*10);
            }
            break;
        }

