#include <stdio.h>
#include <ctype.h>
#include <stdbool.h>
#include <stdlib.h>
#include <time.h>

int main(void)
{
    //Variable
    char Start;
    int ID;
    int MAX = 46;
    const unsigned int Dispaly = 1;
    time_t Begin;
    time_t Seed = 37;
    int Tries = 1;
    int Mark = 0;
    bool BUG = 1;

    //Forward
    printf("Hello,dear Project Alpha Insider!\n\a");
    printf("VoT 37 is able to help you check and memorize words in each unit of "
    "NEW SENIOR ENGLISH FOR CHINAE STUDENT'S BOOK 1.\n");
    printf("VoT 37 will keep on displaying word till you make mistake,this is for \'PRECTICE"
    "MAKES PERFECT\',if you get tired of it,you can just click CLOSE.\n");
    printf("Press Enter to start!");
    scanf("%c",&Start);
    
    //ID
    srand(time(&Seed));
    Core:
    ID = rand() % MAX;
    if(Tries == 1)
    {
        printf("You will see one word stay for just one second and then disapper,if you get ready press Enter to go!\n");
        scanf("%c",&Start);
    }
    else
    {
        if(BUG)
        {
            printf("Go on,practice makes perfect!\n");
            scanf("%c",&Start);
        }
        else
        {
            printf("Now start again,do not worry,your mark is saved,press Enter to go.");
            scanf("%c",&Start);
        }
    }

